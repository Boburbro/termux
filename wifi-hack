import string

from random import *



letters = string.ascii_letters 

digits = string.digits 


chars = letters + digits



min_len = 8

max_len = 16

password = "".join(choice(chars) for x in range(randint(min_len, max_len)))
print ("=========================================")

print("Parol bu",password)
